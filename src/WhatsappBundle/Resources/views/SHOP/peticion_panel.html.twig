                        <div role="tabpanel" class="tab-pane {%if activeTab == "conversations"%}active{%endif%}" id="conversations">
                            <div class="box-header">
                                <h4 class="box-title"> {{ 'Conversaciones'|trans({}, 'WhatsappBundle') }} </h4>

                            </div>          
                            <div class="box-body">

                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="box-body table-responsive no-padding">
                                            <table class="table table-bordered table-striped sonata-ba-list">
                                                <thead>
                                                    <tr class="sonata-ba-list-field-header">
                                                        <th class="sonata-ba-list-field-header-">
                                                            {{ 'Fecha'|trans({}, 'WhatsappBundle') }}
                                                        </th>
                                                        <th class="sonata-ba-list-field-header-">
                                                            {{ 'Teléfonos'|trans({}, 'WhatsappBundle') }}
                                                        </th>
                                                        <th class="sonata-ba-list-field-header-">
                                                            {{ 'Tipo de conversación'|trans({}, 'WhatsappBundle') }}
                                                        </th>
                                                        <th class="sonata-ba-list-field-header-">
                                                            {{ 'Descripcion'|trans({}, 'WhatsappBundle') }}
                                                        </th>
                                                        <th class="sonata-ba-list-field-header-">
                                                            {{ 'Recursos'|trans({}, 'WhatsappBundle') }}
                                                        </th>
                                                    </tr>
                                                </thead>

                                                <tbody>
                                                    {% for conversation in peticion.conversations%}
                                                        <tr class="">
                                                            <td class="">
                                                                {{conversation.day | date("d/m/y h:i")}}
                                                            </td>
                                                            <td class="">
                                                                {{conversation.phone}}
                                                            </td>
                                                            <td class="">
                                                                {{conversation.conversationType}}
                                                            </td>
                                                            <td class="">
                                                                {{conversation.description}}
                                                            </td>
                                                            <td class="">
                                                                {% for file in conversation.files%}
                                                                    <a class="btn btn-info btn-sm btn-block"
                                                                       href="{{ path('sonata_media_download', {'id': (file.media.id)}) }}">
                                                                        {{file.media.name}}
                                                                    </a>
                                                                    {%if file.fileType == "Sonido"%}
                                                                    <audio src="{% path file.media, 'reference' %}" controls="controls" type="audio/wave" preload="preload">
                                                                    </audio>
                                                                    {%elseif file.fileType == "Imagen"%}
                                                                        <div class="text-center">
                                                                            <img class="img-responsive center-block" src="{{ path('sonata_media_download', {'id': (file.media.id)}) }}">
                                                                        </div>
                                                                    {%elseif file.fileType == "Video"%}
                                                                        <video src="{% path file.media, 'reference' %}" controls="controls" type="video/mp4" preload="preload">
                                                                        </video>
                                                                    {%endif%}
                                                                {%endfor%}
                                                            </td>

                                                        </tr>
                                                    {%endfor%}
                                                </tbody>

                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
